package Quests;

public enum JabarsQuestions {
	questionOne;
	public String question(){
		switch(this){
		case questionOne:
			return "What is black and white and loves cold weather.";
		default:
			break;
		}
		return null;
	}
	public String answer(){
		switch(this){
		case questionOne:
			return "Penguins";
		default:
			break;
		}
		return null;
	}
}

