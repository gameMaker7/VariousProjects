package Quests;

public enum JabarsQuestions {
	questionOne, QuestionTwo;
	public String question(){
		switch(this){
		case questionOne:
			return "What is black and white and loves cold weather.";
		case QuestionTwo:
			return "Does this plunger make me look fat";
		default:
			break;
		}
		return null;
	}
	public String answer(){
		switch(this){
		case questionOne:
			return "Penguins";
		case QuestionTwo:
			return "yes";
		default:
			break;
		}
		return null;
	}
}

