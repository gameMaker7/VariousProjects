package recursion;

import java.util.Scanner;

public class something {
	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("Enter n");
		int N = input.nextInt();
		System.out.println(sum(N));
	}

	private static int sum(int n) {
		if(n == 1){
			return 1;
		}else{
		return sum(n-1);
		}
	}
}
